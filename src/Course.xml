        <action name="show_info" class="manage.action.e_manageAction">
        	<result name="success">/WEB-INF/arrageJK/e_info_information.jsp</result>
        </action>
        <action name="show_M_info" class="manage.action.m_manageAction">
        	<result name="success">/WEB-INF/arrageJK/m_info_information.jsp</result>
        </action>
    </package> 
    <package name="uploadCourse" namespace="/uploadCourse" extends="struts-default"> 
    	<action name="upload" class="manage.action.courseAction">  
