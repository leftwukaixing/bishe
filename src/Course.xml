        <action name="showSelf" class="manage.action.e_manageAction">  
            <result name="success">/WEB-INF/arrageJK/arrangejk.jsp</result>  
        </action>
        <action name="showSelfM" class="manage.action.m_manageAction">  
            <result name="success">/WEB-INF/arrageJK/arrangejkM.jsp</result>  
