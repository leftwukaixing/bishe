        </action>
        
        <action name="uploadMExam">  
            <result name="success">/WEB-INF/arrageJK/uploadMExam.jsp</result>  
        </action>
        
        <action name="seeM_info"  class="manage.action.m_infoAction">  
            <result name="success">/WEB-INF/arrageJK/seeM_info.jsp</result>  
        </action>
        <action name="seeE_info"  class="manage.action.e_infoAction">  
            <result name="success">/WEB-INF/arrageJK/seeE_info.jsp</result>  
        </action>
        <action name="seejkTime" class="manage.action.e_manageAction">  
            <result name="success">/WEB-INF/arrageJK/seejkTime.jsp</result>  
