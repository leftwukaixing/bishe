<?xml version="1.0" encoding="utf-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="manage.dao.m_manageDao">  
    <select id="select_m_manage"  parameterType="String" resultType="manage.entity.M_Manage">  
		select * from m_manage where tno_one = #{tno} or tno_two=#{tno} order by timestart
    </select>  
    <insert id="insert_m_manage"  parameterType="manage.entity.M_Manage">
    	insert into m_manage values
    	(#{teacher,jdbcType=VARCHAR},#{cname,jdbcType=VARCHAR},#{timeStart,jdbcType=VARCHAR},
    	#{timeEnd,jdbcType=VARCHAR},#{address,jdbcType=VARCHAR},#{tno_one,jdbcType=VARCHAR},
    	#{tname_one,jdbcType=VARCHAR},#{tel_one,jdbcType=VARCHAR},#{tno_two,jdbcType=VARCHAR},
    	#{tname_two,jdbcType=VARCHAR},#{tel_two,jdbcType=VARCHAR})
    </insert>
    <select id="select_all_m_manage" resultType="manage.entity.M_Manage">
    	select * from m_manage order by timestart
    </select>
    <update id="update_M_Manage" parameterType="manage.entity.M_Manage">
    	update m_manage set tno_one=#{tno_one},tname_one=#{tname_one},
    		tel_one=#{tel_one},tno_two=#{tno_two},tname_two=#{tname_two},tel_two=#{tel_two}
    		where cname=#{cname,jdbcType=VARCHAR} and timeStart=#{timeStart,jdbcType=VARCHAR} and
    		address=#{address,jdbcType=VARCHAR}
 	</update>
    
	<delete id="delete_all_m_manage">
		delete from m_manage
	</delete>
</mapper>