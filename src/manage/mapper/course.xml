<?xml version="1.0" encoding="utf-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="manage.dao.courseDao">  
 	<insert id="insert_course" parameterType="manage.entity.Course">
 		insert into course values(
 			#{grade},#{term},#{cname},
 			#{day_of_week},#{time},#{week},
 			#{remark},#{tno},#{address},#{student})
 	</insert>
 	<select id="select_course"  parameterType="String" resultType="manage.entity.Course">  
		select * from course where tno = #{tno}
    </select> 
    <delete id="delete_all_course">
    	delete from course
    </delete>
</mapper>